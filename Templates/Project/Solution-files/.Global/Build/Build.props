<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<DevelopmentStrongNameKeyFileName>StrongName.Development.snk</DevelopmentStrongNameKeyFileName>
		<GlobalDirectoryRelativePath>.Global\</GlobalDirectoryRelativePath>
		<MessageImportance Condition="$(MessageImportance) == ''">High</MessageImportance><!-- High, Low or Normal -->
		<NuGetVerbosity Condition="$(NuGetVerbosity) == ''">Normal</NuGetVerbosity><!-- Normal, Quiet or Detailed -->
		<TestProjectTypeGuid>3AC096D0-A1C2-E12C-1390-A8335801FDAB</TestProjectTypeGuid>
	</PropertyGroup>
	<PropertyGroup>
		<GlobalCodeAnalysisDirectoryRelativePath>$(GlobalDirectoryRelativePath)CodeAnalysis\</GlobalCodeAnalysisDirectoryRelativePath>
		<GlobalDocumentsDirectoryRelativePath>$(GlobalDirectoryRelativePath)Documents\</GlobalDocumentsDirectoryRelativePath>
		<GlobalPropertiesDirectoryRelativePath>$(GlobalDirectoryRelativePath)Properties\</GlobalPropertiesDirectoryRelativePath>
		<GlobalSigningDirectoryRelativePath>$(GlobalDirectoryRelativePath)Signing\</GlobalSigningDirectoryRelativePath>
	</PropertyGroup>
</Project>